{
  "version": 2,
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "GEMINI_API_KEY_VISION": "@gemini_api_key_vision"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "github": {
    "silent": true
  }
} 